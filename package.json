{
  "name": "ultra-fast-arbitrage-system",
  "version": "2.0.0",
  "description": "Ultra-Fast BSC Arbitrage System with Advanced MEV Protection and Performance Optimization",
  "main": "scripts/ultra-fast-arbitrage.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist && rm -rf cache && rm -rf artifacts",
    "compile": "hardhat compile",
    "compile:ultra": "tsc scripts/ultra-*.ts --target ES2020 --module ESNext --moduleResolution node",
    "ultra:start": "ts-node --esm scripts/ultra-fast-arbitrage.ts",
    "ultra:mainnet": "NETWORK=mainnet ts-node --esm scripts/ultra-fast-arbitrage.ts",
    "ultra:testnet": "NETWORK=testnet ts-node --esm scripts/ultra-fast-arbitrage.ts",
    "ultra:monitor": "ts-node --esm scripts/ultra-fast-monitor.ts",
    "ultra:monitor:mainnet": "NETWORK=mainnet ts-node --esm scripts/ultra-fast-monitor.ts",
    "ultra:monitor:testnet": "NETWORK=testnet ts-node --esm scripts/ultra-fast-monitor.ts",
    "ultra:execute": "ts-node --esm scripts/ultra-execution-engine.ts",
    "ultra:execute:mainnet": "NETWORK=mainnet ts-node --esm scripts/ultra-execution-engine.ts",
    "ultra:execute:testnet": "NETWORK=testnet ts-node --esm scripts/ultra-execution-engine.ts",
    "ultra:gas-optimize": "ts-node --esm scripts/ultra-gas-optimizer.ts",
    "ultra:mempool": "ts-node --esm scripts/ultra-mempool-manager.ts",
    "ultra:oracle": "ts-node --esm scripts/ultra-price-oracle.ts",
    "ultra:contracts": "ts-node --esm scripts/ultra-contract-interface.ts",
    "ultra:test": "npm run ultra:testnet -- --test-mode",
    "ultra:performance": "ts-node --esm scripts/performance-test.ts",
    "ultra:stress": "ts-node --esm scripts/stress-test.ts",
    "ultra:optimize": "ts-node --esm scripts/auto-optimizer.ts",
    "deploy:ultra": "hardhat run scripts/deploy-ultra-fast.ts --network bscTestnet",
    "deploy:ultra:mainnet": "hardhat run scripts/deploy-ultra-fast.ts --network bsc",
    "deploy:contracts": "hardhat compile && npm run deploy:ultra",
    "deploy:ultra-secure": "ts-node --esm scripts/deploy-ultra-secure.ts",
    "deploy:ultra-secure:mainnet": "NETWORK=mainnet ts-node --esm scripts/deploy-ultra-secure.ts",
    "deploy:ultra-secure:testnet": "NETWORK=testnet ts-node --esm scripts/deploy-ultra-secure.ts",
    "start:dashboard": "ts-node --esm scripts/ultra-secure-dashboard.ts",
    "start:dashboard:mainnet": "NETWORK=mainnet ts-node --esm scripts/ultra-secure-dashboard.ts",
    "start:dashboard:testnet": "NETWORK=testnet ts-node --esm scripts/ultra-secure-dashboard.ts",
    "start:arbitrage": "ts-node --esm scripts/ultra-fast-arbitrage.ts",
    "start:arbitrage:mainnet": "NETWORK=mainnet ts-node --esm scripts/ultra-fast-arbitrage.ts",
    "start:arbitrage:testnet": "NETWORK=testnet ts-node --esm scripts/ultra-fast-arbitrage.ts",
    "security:monitor": "ts-node --esm scripts/security-monitor.ts",
    "security:layer": "ts-node --esm scripts/security-layer.ts",
    "emergency:pause": "ts-node --esm scripts/emergency-pause.ts",
    "emergency:withdraw": "ts-node --esm scripts/emergency-withdraw.ts",
    "test:integration": "ts-node --esm scripts/test-ultra-secure-integration.ts",
    "test:integration:mainnet": "NETWORK=mainnet ts-node --esm scripts/test-ultra-secure-integration.ts",
    "test:integration:testnet": "NETWORK=testnet ts-node --esm scripts/test-ultra-secure-integration.ts",
    "test:security": "npm run test:integration -- --focus=security",
    "test:performance": "npm run test:integration -- --focus=performance",
    "test:stress": "npm run test:integration -- --focus=stress",
    "stats": "ts-node --esm -e \"import('./scripts/ultra-fast-arbitrage.js').then(m => m.getStats())\"",
    "stats:performance": "ts-node --esm -e \"import('./scripts/ultra-fast-arbitrage.js').then(m => m.getPerformanceMetrics())\"",
    "stats:security": "ts-node --esm -e \"import('./scripts/ultra-fast-arbitrage.js').then(m => m.getSecurityMetrics())\"",
    "verify:testnet": "hardhat verify --network bscTestnet",
    "verify:mainnet": "hardhat verify --network bsc",
    "lint": "eslint scripts/**/*.ts",
    "lint:fix": "eslint scripts/**/*.ts --fix",
    "format": "prettier --write 'scripts/**/*.ts' 'contracts/**/*.sol'",
    "type-check": "tsc --noEmit --skipLibCheck",
    "dev": "npm run ultra:testnet",
    "start": "npm run ultra:mainnet",
    "test": "npm run ultra:test",
    "prepare": "npm run build"
  },
  "keywords": [
    "bsc",
    "arbitrage",
    "flash-loan",
    "defi",
    "mev-protection",
    "ultra-fast",
    "performance",
    "optimization",
    "binance-smart-chain",
    "web3",
    "ethereum",
    "trading-bot",
    "automated-trading"
  ],
  "author": "Ultra Arbitrage Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/ultra-fast-arbitrage.git"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox-viem": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.19.9",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.26.2",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^5.0.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "big.js": "^6.2.1",
    "bignumber.js": "^9.1.2",
    "bottleneck": "^2.19.5",
    "bull": "^4.16.3",
    "bullmq": "^5.15.0",
    "chalk": "^5.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "ethers": "^6.15.0",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "inquirer": "^10.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.1",
    "moment": "^2.30.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "ora": "^8.0.1",
    "p-limit": "^5.0.0",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "viem": "^2.33.3",
    "winston": "^3.17.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "hardhat": "^2.22.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "node >= 18"
  ],
  "files": [
    "scripts/",
    "contracts/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "bugs": {
    "url": "https://github.com/your-repo/ultra-fast-arbitrage/issues"
  },
  "homepage": "https://github.com/your-repo/ultra-fast-arbitrage#readme"
}
